{
    "github": {
        "silent": true
    },
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "redirects": [
        {
            "source": "/api(.*)",
            "destination": "https://api-bub-it.vercel.app"
        }
    ]
}