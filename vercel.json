{
    "github": {
        "silent": true
    },
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "redirects": [
        {
            "source": "/api/:path*",
            "destination": "https://api-bub-it.vercel.app/api/:path*"
        }
    ]
}